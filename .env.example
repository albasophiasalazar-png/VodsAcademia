# Configuración de ejemplo para VodsAcademia
# Copia este archivo a .env y configura tus valores

# Base de datos (por defecto usa SQLite)
DATABASE_PATH=database/vodsacademia.db

# Configuración de Streamlit
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_HEADLESS=true

# Usuario admin por defecto (cambiar después del primer uso)
DEFAULT_ADMIN_USER=admin
DEFAULT_ADMIN_PASSWORD=admin123
